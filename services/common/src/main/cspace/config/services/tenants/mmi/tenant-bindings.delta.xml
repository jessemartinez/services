<?xml version="1.0" encoding="UTF-8"?>
<tenant:TenantBindingConfig
    xmlns:merge='http://xmlmerge.el4j.elca.ch'
    xmlns:tenant='http://collectionspace.org/services/config/tenant'>
             
    <tenant:tenantBinding id="42" name="movingimage.us" displayName="CollectionSpace Core MMI Tenant" version="0.1">
        <tenant:repositoryDomain name="default-domain" storageName="mmi-domain" repositoryClient="nuxeo-java" />

        <tenant:serviceBindings merge:matcher="id" id="CollectionObjects">
            <service:DocHandlerParams xmlns:service="http://collectionspace.org/services/config/service">
                <service:params>
                    <service:ListResultsFields>
                        <service:ListResultField merge:matcher="skip" merge:action="insert">
                            <service:element>caption</service:element>
                            <service:schema>collectionobjects_mmi</service:schema>
                            <service:xpath>caption</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField merge:matcher="skip" merge:action="insert">
                            <service:element>artifactClass</service:element>
                            <service:schema>collectionobjects_mmi</service:schema>
                            <service:xpath>artifactClass</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField merge:matcher="skip" merge:action="insert">
                            <service:element>workType</service:element>
                            <service:schema>collectionobjects_mmi</service:schema>
                            <service:xpath>workType</service:xpath>
                        </service:ListResultField>
                    </service:ListResultsFields>
                </service:params>
            </service:DocHandlerParams>
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="42" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="collectionobjects_mmi" updated="" order="3">
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/collectionobject/local/mmi" schemaLocation="http://collectionspace.org/services/collectionobject/local/mmi http://collectionspace.org/services/collectionobject/local/collectionobjects_mmi.xsd" />
                    </service:content>
                    <service:properties>
                        <!-- See CSPACE-3938 for potential additional authRef and termRef entries -->
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>authRef</types:key>
                              <types:value>contentWorkList/*/contentWork</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>authRef</types:key>
                              <types:value>contentEntityList/*/contentEntity</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>authRef</types:key>
                              <types:value>objProdEntityList/*/objProdEntity</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>contentWorkList/*/contentWorkType</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>contentEntityList/*/contentEntityType</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>objProdEntityList/*/objProdEntityRole</types:value>
                        </types:item>
                    </service:properties>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Conditionchecks">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="42" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="conditionchecks_mmi" updated="" order="3">
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/conditioncheck/local/mmi" schemaLocation="http://collectionspace.org/services/conditioncheck/local/mmi http://collectionspace.org/services/conditioncheck/local/conditionchecks_mmi.xsd" />
                     </service:content>
                     <service:properties>
                        <!-- See CSPACE-3938 for potential additional authRef and termRef entries -->
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>technicalChangeList/*/technicalChange</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>technicalChangeList/*/technicalChangeReason</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>technicalChangeList/*/previousSupport</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>technicalChangeList/*/newSupport</types:value>
                        </types:item>
                    </service:properties>
                </service:part>
            </service:object>
        </tenant:serviceBindings>


        <tenant:serviceBindings merge:matcher="id" id="Media">
            <service:DocHandlerParams xmlns:service="http://collectionspace.org/services/config/service">
                <service:params>
                    <service:ListResultsFields>
                        <service:ListResultField merge:matcher="skip" merge:action="insert" >
                            <service:element>isPrimary</service:element>
                            <service:schema>media_mmi</service:schema>
                            <service:xpath>isPrimary</service:xpath>
                        </service:ListResultField>
                    </service:ListResultsFields>
                </service:params>
            </service:DocHandlerParams>
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="42" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="media_mmi" updated="" order="3">
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/media/local/mmi" schemaLocation="http://collectionspace.org/services/media/local/mmi http://collectionspace.org/services/media/local/media_mmi.xsd" />
                    </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Persons">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="42" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="persons_mmi" updated="" order="3">
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/person/local/mmi" schemaLocation="http://collectionspace.org/services/person/local/mmi http://collectionspace.org/services/person/local/persons_mmi.xsd" />
                    </service:content>
                    <service:properties>
                        <!-- See CSPACE-3938 for potential additional authRef and termRef entries -->
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>authRef</types:key>
                              <types:value>affiliatedPersonOrgGroupList/*/affiliatedPersonOrg</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>authRef</types:key>
                              <types:value>orgContactNames|orgContactName</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                              <types:key>termRef</types:key>
                              <types:value>affiliatedPersonOrgGroupList/*/affiliatedPersonOrgType</types:value>
                        </types:item>
                    </service:properties>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Acquisitions">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="42" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="acquisitions_mmi" updated="" order="3">
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/acquisition/local/mmi" schemaLocation="http://collectionspace.org/services/acquisition/local/mmi http://collectionspace.org/services/acquisition/local/acquisitions_mmi.xsd" />
                     </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

    </tenant:tenantBinding>
                   
</tenant:TenantBindingConfig>
